{
  "name": "@dazejs/framework",
  "version": "2.0.2",
  "description": "Daze.js - A powerful web framework for Node.js",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "test": "jest --verbose --runInBand",
    "copy": "copyfiles -u 1 src/**/*.njk dist",
    "build": "npm run clean && npm run copy && tsc",
    "dev": "npm run clean && npm run copy &&  tsc -w",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "test:coverage": "jest --detectOpenHandles --runInBand --forceExit --coverage",
    "debugger": "node --inspect-brk node_modules/.bin/jest --runInBand --no-cache --no-watchman --detectOpenHandles"
  },
  "keywords": [
    "dazejs",
    "framework",
    "nodejs",
    "node",
    "api",
    "restful",
    "es6",
    "nodeframework",
    "web",
    "application",
    "http",
    "app",
    "middleware",
    "provider",
    "decractor"
  ],
  "author": "Zewail Chan",
  "contributors": [
    {
      "name": "chan zewail",
      "email": "chanzewail@gmail.com"
    }
  ],
  "homepage": "https://dazejs.org/",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dazejs/daze"
  },
  "bugs": {
    "url": "https://github.com/dazejs/daze/issues"
  },
  "engines": {
    "node": ">=10.15.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "readmeFilename": "README.md",
  "dependencies": {
    "@dazejs/trace-page": "^0.3.2",
    "accepts": "^1.3.7",
    "axios": "^0.19.0",
    "body-parser": "^1.18.3",
    "cache-content-type": "^1.0.1",
    "co-body": "^6.0.0",
    "compressible": "^2.0.17",
    "content-disposition": "^0.5.3",
    "cookies": "^0.8.0",
    "crc": "^3.8.0",
    "csrf": "^3.0.6",
    "date-fns": "^2.8.1",
    "debug": "^4.1.1",
    "formidable": "^1.2.1",
    "glob": "^7.1.3",
    "keygrip": "^1.1.0",
    "klaw-sync": "^6.0.0",
    "lodash": "^4.17.11",
    "mime": "^2.4.0",
    "mime-types": "^2.1.24",
    "minimatch": "^3.0.4",
    "mysql": "^2.17.1",
    "nunjucks": "^3.1.3",
    "parseurl": "^1.3.2",
    "path-to-regexp": "^6.1.0",
    "pluralize": "^8.0.0",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "statuses": "^1.5.0",
    "string-hash": "^1.1.3",
    "type-is": "^1.6.16",
    "uuid": "^3.3.2",
    "validator": "^12.0.0",
    "vary": "^1.1.2",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.1.0",
    "winston-mongodb": "^5.0.0"
  },
  "devDependencies": {
    "@types/compressible": "^2.0.0",
    "@types/mysql": "^2.15.7",
    "@types/pluralize": "^0.0.29",
    "copyfiles": "^2.1.1",
    "rimraf": "^3.0.0"
  }
}
