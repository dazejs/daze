    "@types/jest": "25.1.4",
    "@types/koa": "2.11.2",
    "@types/koa-router": "7.4.0",
    "@types/node": "14.0.26",
    "@types/supertest": "2.0.8",
    "@types/type-is": "1.6.3",
    "@types/uuid": "7.0.2",
    "@types/validator": "12.0.1",
    "@typescript-eslint/eslint-plugin": "5.46.0",
    "@typescript-eslint/parser": "5.46.0",
    "autocannon": "4.6.0",
    "copyfiles": "2.2.0",
    "cross-env": "6.0.0",
