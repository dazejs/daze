    "husky": "4.3.8",
    "jest": "29.3.1",
    "koa": "2.11.0",
    "koa-router": "8.0.8",
    "lerna": "6.4.0",
    "markdown-table": "2.0.0",
    "rimraf": "3.0.2",
    "supertest": "4.0.2",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.9.4"
